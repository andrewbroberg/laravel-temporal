{
    "name": "keepsuit/laravel-temporal",
    "description": "Laravel temporal.io",
    "keywords": [
        "keepsuit",
        "laravel",
        "laravel-temporal"
    ],
    "homepage": "https://github.com/keepsuit/laravel-temporal",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Capucci",
            "email": "f.capucci@keepsuit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer/class-map-generator": "^1.0",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "spiral/roadrunner": "^2023.2.0",
        "spiral/roadrunner-cli": "^2.5",
        "symfony/process": "^6.0",
        "temporal/sdk": "^2.6.1"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^0.26.0",
        "laravel/pint": "^1.0",
        "nesbot/carbon": "^2.63.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "rector/rector": "^0.18.2",
        "spatie/invade": "^2.0",
        "spatie/laravel-data": "^3.0",
        "spatie/laravel-ray": "^1.26"
    },
    "suggest": {
        "spatie/laravel-data": "Can be used for workflows payloads (^3.0)"
    },
    "autoload": {
        "psr-4": {
            "Keepsuit\\LaravelTemporal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keepsuit\\LaravelTemporal\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/roadrunner-temporal-worker"
    ],
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "test": "pest",
        "test-coverage": "pest --coverage",
        "format": "pint",
        "lint": "pint && rector process --dry-run",
        "lint:fix": "pint && rector process"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keepsuit\\LaravelTemporal\\LaravelTemporalServiceProvider"
            ]
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
